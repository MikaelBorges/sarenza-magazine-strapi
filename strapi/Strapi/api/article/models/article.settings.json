{
  "kind": "collectionType",
  "collectionName": "Edito",
  "info": {
    "name": "article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "url": {
      "type": "uid",
      "targetField": "title"
    },
    "author": {
      "type": "string"
    },
    "image": {
      "type": "string"
    },
    "ImageArticleMobile": {
      "type": "string"
    },
    "subtitle": {
      "type": "string"
    },
    "module": {
      "type": "dynamiczone",
      "components": [
        "media.image",
        "module.slider-article",
        "module.paragraphe",
        "module.duo",
        "media.video",
        "module.block-edito-long",
        "module.ligne-produit",
        "module.produit-edito",
        "module.duo-qr",
        "module.identity-card",
        "module.trio-qr",
        "module.product-look",
        "module.images-legend",
        "module.duo-image"
      ]
    },
    "rubriques": {
      "collection": "rubrique",
      "via": "articles",
      "dominant": true
    },
    "tags": {
      "collection": "tag",
      "via": "articles",
      "dominant": true
    },
    "isHeadline": {
      "type": "boolean",
      "default": false
    },
    "isSeo": {
      "type": "boolean",
      "default": false
    }
  }
}
